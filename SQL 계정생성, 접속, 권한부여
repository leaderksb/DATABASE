sys AS SYSDBA
-- 계정생성
CREATE USER C##tokki IDENTIFIED BY 비밀번호;
-- 권한부여
GRANT CREATE SESSION TO tokki;
GRANT CREATE ANY TABLE TO tokki;
ALTER SESSION SET "_ORACLE_SCRIPT" = TRUE;
CREATE TABLE topic(
  id NUMBER NOT NULL,
  title VARCHAR2(50) NOT NULL,
  description VARCHAR2(4000),
  created DATE NOT NULL
);
SELECT table_name FROM all_tables WHERE OWNER = 'C##TOKKI';
